{
  "name": "highscore",
  "version": "1.3.0",
  "description": "Grollmus Highscore-Board to count Creative-Day points.",
  "author": "Grollmus München GmbH <developers@grollmus.de> (https://www.grollmus.de/)",
  "maintainers": [
    "Daniel Weiß <daniel.weiss@grollmus.de> (https://www.grollmus.de)"
  ],
  "contributors": [
    "Patrick Henkelmann"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ng:start": "ng serve",
    "ng:start:live-backend": "ng serve --configuration=production",
    "ng:build": "ng build --prod --aot",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "ng e2e",
    "test": "jest",
    "nest:build": "tsc -p tsconfig.build.json",
    "nest:format": "prettier --write \"src/**/*.ts\"",
    "nest:start": "ts-node -r tsconfig-paths/register src/main.ts",
    "nest:start:dev": "nodemon",
    "nest:start:debug": "nodemon --config nodemon-debug.json",
    "nest:prestart:prod": "rimraf dist && npm run build",
    "nest:start:prod": "node dist/main.js",
    "nest:lint": "tslint -p tsconfig.json -c tslint.json",
    "nest:test:watch": "jest --watch",
    "nest:test:cov": "jest --coverage",
    "nest:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/swagger": "^3.1.0",
    "bcrypt": "^3.0.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "core-js": "^3.6.4",
    "dialog-polyfill": "^0.5.0",
    "faker": "^4.1.0",
    "foundation-sites": "^6.6.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.12",
    "nes.css": "^2.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "~6.5.4",
    "swagger-ui-express": "^4.1.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@nestjs/testing": "^6.10.1",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/jest": "^26.0.3",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^13.9.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "codelyzer": "^5.1.2",
    "jest": "^26.1.0",
    "jest-preset-angular": "^8.2.1",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "~8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "pre-push": "npm test -- --watch=false --browsers=ChromeHeadless"
    }
  }
